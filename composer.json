{
	"require": {
		"aelvan/imager": "v2.4.0",
		"anubarak/craft-relabel": "1.3.4",
		"craftcms/cms": "3.4.11",
		"craftcms/element-api": "2.6.0",
		"craftcms/redactor": "2.6.1",
		"ether/seo": "3.6.3",
		"ether/splash": "3.0.5",
		"nystudio107/craft-eagerbeaver": "1.0.4",
		"sebastianlenz/linkfield": "1.0.20",
		"vaersaagod/matrixmate": "1.2.5",
		"verbb/smith": "1.1.1",
		"vlucas/phpdotenv": "^3.4.0",
		"yiisoft/yii2-redis": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"modules\\pwamodule\\": "modules/pwamodule/src/"
		}
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true,
		"platform": {
			"php": "7.0"
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		]
	}
}
